{
    "help": {
        "alias": [ "halp" ],
        "endpoint": "#",
        "help": "help [command](optional)",
        "description": "To get help on all supported commands, or a specified command"
    },
	"order": {
		"alias": [],
		"endpoint": "#",
		"help": "order [restaurant-id]",
		"description": "Starts an order request to the restaurant name"
	},
    "order-check": {
        "alias": [],
        "endpoint": "#",
        "help": "order-check [username]",
        "description": "Checks an open order of a specific username"
    },
    "order-join": {
        "alias": [],
        "endpoint": "#",
        "help": "order-join [username] [item-id] [quantity] [obs](optional)",
        "description": "Adds a request to an open order"
    },
    "order-custom": {
        "alias": [],
        "endpoint": "#",
        "help": "order-custom [username] [item-label] [price]",
        "description": "Adds a custom request to an open order"
    },
    "order-shared": {
        "alias": [],
        "endpoint": "#",
        "help": "order-shared [item-id] [quantity] [obs](optional)",
        "description": "Adds a request to everyone in an open order"
    },
    "order-delete": {
        "alias": [],
        "endpoint": "#",
        "help": "order-delete",
        "description": "Removes the open order that you made"
    },
    "order-unjoin": {
        "alias": [],
        "endpoint": "#",
        "help": "order-unjoin [username] [item-id] [quantity]",
        "description": "Removes a request from an open order"
    },
    "set-delivery-price": {
        "alias": [],
        "endpoint": "#",
        "help": "set-delivery-price [price]",
        "description": "Sets the delivery price"
    },
    "order-close": {
        "alias": [],
        "endpoint": "#",
        "help": "order-close",
        "description": "Closes your open order"
    },
    "menu": {
        "alias": [],
        "endpoint": "#",
        "help": "menu [restaurant-id]",
        "description": "Checks the restaurant's menu"
    },
    "restaurants": {
        "alias": [],
        "endpoint": "#",
        "help": "restaurants",
        "description": "Gets all restaurants and ids"
    },
	"error": {
		"exclude": true
	}
}